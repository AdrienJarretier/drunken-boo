	"scripts": {
		"EFB1376E-5B88-4D74-89C8-8D6926B33CAA": [
			{
				"name": "move",
				"source": "var controls = {\n\tleft:37,\n\tright:39,\n\tup:38,\n\tdown:40,\n}\nvar keys = {}\nfor(var i = 0;i < 200;i++){ keys[i] = false; }\n\nfunction keydown( event ){\n\tkeys[event.keyCode] = true;\n}\n\nfunction keyup( event ){\n\tkeys[event.keyCode] = false;\n}\n\nfunction update( event ) {\n\tvar direction = new THREE.Vector3();\n\tdirection.x = keys[controls.left] ? 1 : keys[controls.right] ? -1 : 0;\n\tdirection.z = keys[controls.up] ? 1 : keys[controls.down] ? -1 : 0;\n\t//console.log(direction);\n\tthis.translateZ( direction.z*10 );\n\tthis.rotateY( direction.x*0.1 );\n\t\n\tvar pt = this.parent.getObjectByName('Points').getObjectByName('P1');\n\tvar dist = pt.position.distanceTo(this.position);\n\t//console.log(dist);\n\tif(dist < 100){\n\t\tif(pt.visible){\n\t\t\tpt.visible = false;\n\t\t}\n\t}\n}"
			}],
